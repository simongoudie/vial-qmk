            { "pin_a": "B4", "pin_b": "B5", "resolution": 4 }
        ]
    },
    "layouts": {
        "LAYOUT_numpad_1x5": {
            "layout": [
                { "matrix": [0, 0], "x": 0, "y": 0 },
                { "matrix": [0, 1], "x": 1, "y": 0 },
                { "matrix": [0, 2], "x": 2, "y": 0 },
                { "matrix": [0, 3], "x": 3, "y": 0 },
                { "matrix": [0, 4], "x": 4, "y": 0 }
            ]
        }
    }
